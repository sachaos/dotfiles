# change focus
cmd - h : yabai -m window --focus west
cmd - j : yabai -m window --focus south
cmd - k : yabai -m window --focus north
cmd - l : yabai -m window --focus east

# move focused window
cmd + ctrl - h : yabai -m window --warp west
cmd + ctrl - j : yabai -m window --warp south
cmd + ctrl - k : yabai -m window --warp north
cmd + ctrl - l : yabai -m window --warp east

# move window above monitor
cmd + ctrl + shift - h : yabai -m window --display 2; yabai -m display --focus 2
cmd + ctrl + shift - l : yabai -m window --display 1; yabai -m display --focus 1
# move monitor focus
cmd + shift - h : yabai -m display --focus 2
cmd + shift - l : yabai -m display --focus 1

cmd - y : yabai -m window --toggle zoom-fullscreen

# increase window size
alt - h : yabai -m window --resize left:-20:0
alt - j : yabai -m window --resize bottom:0:20
alt - k : yabai -m window --resize top:0:-20
alt - l : yabai -m window --resize right:20:0